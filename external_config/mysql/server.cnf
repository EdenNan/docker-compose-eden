[mysqld]
server_id = 1
log-bin=master
binlog_format=row
binlog_row_image=FULL

character-set-server=utf8
collation-server=utf8_unicode_ci

character-set-client-handshake=0

default-storage-engine=INNODB

log_bin_trust_function_creators=1

expire_logs_days=99
sync_binlog=0

[mysql]
default_character_set=utf8